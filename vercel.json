{
    "version": 2,
    "builds":[
        {
            "src":"server.js",
            "use": "@vercel/node"
        }
    ],
        "routes": [
            {
                "src": "/assets/css/(.*)",
      "dest": "/assets/css/$1"
            },
            {
                "src": "/assets/images/(.*)",
      "dest": "/assets/images/$1"
            },
            {
                "src": "/assets/js/(.*)",
      "dest": "/assets/js/$1"
            },
            {
                "src":"/(.*)",
                "dest": "server.js"
            }
        ]
    
}